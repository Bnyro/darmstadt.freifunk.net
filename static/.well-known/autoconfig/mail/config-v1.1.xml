    <displayName>darmstadt.freifunk.net</displayName>
    <displayShortName>Freifunk Darmstadt</displayShortName>
    <incomingServer type="imap">
      <hostname>mail.darmstadt.freifunk.net</hostname>
      <port>993</port>
      <socketType>SSL</socketType>
      <authentication>password-cleartext</authentication>
      <username>%EMAILADDRESS%</username>
